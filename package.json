  "scripts": {
    "lint": "eslint src api",
    "precommit": "yarn lint",
    "prepush": "yarn lint",
    "clean": "rm -rf build api/dist",
    "start": "npm-run-all --parallel start:*",
    "start:client": "react-scripts start",
    "start:server": "nodemon api/index.js -w api",
    "build": "yarn clean && npm-run-all build:*",
    "build:client": "react-scripts build",
    "build:server:debug": "netlify-lambda build api/functions --config api/webpack.functions.js",
    "build:server": "NODE_ENV=production netlify-lambda build api/functions --config api/webpack.functions.js",
    "bundle": "yarn clean && npm-run-all bundle:*",
    "bundle:client": "REACT_APP_API_PREFIX=\"\" REACT_APP_APP_NAME=\"Wallet Playground\" yarn build:client",
    "bundle:server": "DEBUG=@abtnode/* NODE_ENV=production abtnode bundle -w api/webpack.blocklet.js",
    "bundle-server": "DEBUG=@abtnode/* NODE_ENV=development abtnode bundle -w api/webpack.blocklet.js",
    "prepublishOnly": "npm run bundle && rm -f _blocklet/build/**/*.{js,css}.map && node scripts/prepare.js",
    "test": "react-scripts test",
    "echo": "export MESSAGE=$1 && echo $MESSAGE",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "babel-eslint": "^10.0.3",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.18.3",
    "jest": "^24.1.0",
    "lerna": "^3.20.2",
    "netlify-lambda": "^1.4.13",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5"
  },
  "blocklet": {
    "group": "dapp",
    "color": "secondary",
    "logo": "logo.png",
    "main": "api/index.js",
    "provider": "arcblock",
    "admin_url": "",
    "config_url": "",
    "public_url": "/",
    "hooks": {
      "pre-start": "node api/libs/hooks/pre-start.js"
    },
    "hookFiles": [
      "api/libs/hooks/pre-start.js"
    ],
    "requiredEnvironments": [
      {
        "name": "MONGO_URI",
        "description": "Please provide a valid MongoDB connection string to start",
        "required": true,
        "default": ""
      }
    ]
  },
  "license": "Apache-2.0"
}
